a = int(input("Nhap so nguyen a: "))
b = int(input("Nhap so nguyen b: "))
c = int(input("Nhap so nguyen c: "))
d = int(input("Nhap so nguyen d: "))

if a >= c:
    e = a
else:
    e = c
    
if b <= d:
    f = b
else:
    f = d

n = f - e + 1
if n > 0:
    print("co",n,"so nguyen thuoc","[",e,";",f,"]")
else:
    print("Hai tap hop khong giao nhau")
